<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.whalegoods.mapper.DeviceExlogMapper">

 <resultMap id="BaseResultMap" type="com.whalegoods.entity.DeviceExLog">
    <id column="id" jdbcType="VARCHAR" property="id" />
    <result column="orderId" jdbcType="VARCHAR" property="order_id" /> 	
    <result column="errorMessage" jdbcType="VARCHAR" property="error_message" />
    <result column="deviceId" jdbcType="VARCHAR" property="device_id" />
    <result column="short_name" jdbcType="VARCHAR" property="shortName" />
    <result column="goodsName" jdbcType="VARCHAR" property="goods_name" />
    <result column="fileUrl" jdbcType="VARCHAR" property="file_url" />
    <result column="create_by" jdbcType="VARCHAR" property="createBy" />    
    <result column="create_date" jdbcType="TIMESTAMP" property="createDate" />
    <result column="update_by" jdbcType="VARCHAR" property="updateBy" />
    <result column="update_date" jdbcType="TIMESTAMP" property="updateDate" />
    <result column="remark" jdbcType="VARCHAR" property="remark" />
  </resultMap>
  
   <sql id="Base_Column_List">
    a.id,
    a.order_id,
    a.error_message,
    a.device_id,
    b.short_name,
    a.goods_name,
    a.file_url,
    a.create_by,
    a.create_date,
    a.update_by,
    a.update_date,
    a.remark
  </sql>

  <insert id="insert" parameterType="com.whalegoods.entity.DeviceExLog">
    INSERT INTO devcie_exlog (
    id,
    order_id, 
    error_message,
    device_id,
    goods_name,
    file_url,
    create_by,
    create_date,
    update_by,
    update_date)
    VALUES (
    #{id},
    #{order_id}, 
    #{error_message}, 
    #{device_id}, 
    #{goods_name}, 
    #{file_url}, 
    #{createBy}, 
    now(),
    #{updateBy},
    now()
    )
  </insert>
  
  <select id="selectListByObjCdt" parameterType="com.whalegoods.entity.DeviceExLog" resultMap="BaseResultMap">
    SELECT <include refid="Base_Column_List"/> FROM devcie_exlog a INNER JOIN device b ON a.device_id=b.id
    <where>
     <if test="deviceId !=null and deviceId !=''"> AND a.device_id= #{deviceId}</if>
    </where>
  </select>
  
</mapper>
